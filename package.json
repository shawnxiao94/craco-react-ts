{
  "name": "ts-craco-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "antd": "^4.9.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "craco start FAST_REFRESH=true --open",
    "build": "craco build",
    "test": "craco test",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "format": "eslint --fix 'src/**/*.{js,ts,tsx}'",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "react",
    "create-react-app",
    "cra"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue,jsx,ts,tsx,json,css,less,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@craco/craco": "^5.9.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "antd-dayjs-webpack-plugin": "^1.0.1",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-styled-components": "^1.12.0",
    "circular-dependency-plugin": "^5.2.2",
    "craco-antd": "^1.19.0",
    "craco-less": "^1.17.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "simple-progress-webpack-plugin": "^1.1.2",
    "webpack-dashboard": "^3.2.1",
    "webpackbar": "^5.0.0-3"
  }
}
